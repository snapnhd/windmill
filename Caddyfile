:10000 {
    bind 0.0.0.0
    header Strict-Transport-Security "max-age=31536000; includeSubDomains; preload"
    header X-Frame-Options "SAMEORIGIN"
    header Content-Security-Policy "default-src 'self'; script-src 'self'; style-src 'self' 'unsafe-inline'; img-src 'self' data:; connect-src 'self' wss:; frame-ancestors 'none';"
    respond /healthz 200
    reverse_proxy /ws/* {$LSP_ORIGIN}
    reverse_proxy /ws_mp/* {$MULTIPLAYER_ORIGIN}
    reverse_proxy /api/srch/* {$INDEXER_ORIGIN}
    reverse_proxy {$SERVER_ORIGIN}
}
